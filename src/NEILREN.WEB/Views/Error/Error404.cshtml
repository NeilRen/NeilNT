@{
    ViewBag.Title = "404 NOT FOUND";
    Layout = "~/Views/Shared/_NEILRENLayout.cshtml";
}
<ul class="breadcrumb push-down-0">
    <li><a href="/">Home</a></li>
    <li><a href="JavaScript:void(0)">系统错误</a></li>
    <li class="active">错误 404</li>
</ul>
<!-- END BREADCRUMB -->
<!-- PAGE CONTENT WRAPPER -->
<div class="page-content-wrap">

    <div class="row" style="min-height:768px;">
        <div class="col-md-12">

            <div class="error-container">
                <div class="error-code">404</div>
                <div class="error-text">page not found</div>
                <div class="error-subtext">页面未能找到。很遗憾，您访问的地址不存在！您可能访问了错误的地址，或者网站已经将原有页面删除或转移。您可以尝试下方搜索功能。</div>
                <div class="error-actions">
                    <div class="row">
                        <div class="col-md-6">
                            <button class="btn btn-info btn-block btn-lg" onclick="document.location.href = '/';">返回网站首页</button>
                        </div>
                        <div class="col-md-6">
                            <button class="btn btn-primary btn-block btn-lg" onclick="history.back();">返回前一页面</button>
                        </div>
                    </div>
                </div>
                <div class="error-subtext">获取您可以使用搜索来找到您需要的东西。</div>
                <div class="row">
                    <div class="col-md-12">
                        <form action="/Search/">
                            <div class="input-group">
                                <input type="text" name="wd" placeholder="搜索..." class="form-control" />
                                <div class="input-group-btn">
                                    <button class="btn btn-primary"><span class="fa fa-search"></span></button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

        </div>
    </div>
